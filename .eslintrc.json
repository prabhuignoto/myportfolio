{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/prop-types": "off",
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    },
    "env": {
        "jest": true
    }
}